
!probabilistic_rule
{
	u1 ?[v1]'h -> u2 ?[v2]'h :: probability(double_t); 
}

!environment_communication_rule
{
	[[a]'e1 [ ]'e2 ]'p -> [[ ]'e1 [b]'e2 ]'p :: probability(double_t);  
	[[a]'e ]'p -> [[b]'e ]'p :: probability(double_t);  
	[[a]'e ]'p -> [[ ]'e ]'p :: probability(double_t);  
}


@model(pdp) = probabilistic_rule;
